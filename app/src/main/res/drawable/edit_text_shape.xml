<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" android:state_focused="false">
        <shape>
            <gradient
                android:centerY="0.1"

                android:angle="270"
                />
            <stroke
                android:width="0.7dp"
                android:color="@color/colorSecondary"
                />
            <corners
                android:radius="15dp"/>
        </shape>
    </item>
</selector>